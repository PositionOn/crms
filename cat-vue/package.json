{
  "name": "rcms-frontend",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build && electron-builder",
    "build:mac": "vite build && electron-builder --mac",
    "build:win": "vite build && electron-builder --win",
    "preview": "vite preview"
  },
  "build": {
    "appId": "com.rcms.frontend",
    "productName": "管理系统",
    "mac": {
      "target": "dmg",
      "icon": "public/icon.png"
    },
    "win": {
      "target": "nsis",
      "icon": "public/icon.png"
    },
    "files": [
      "dist/**/*",
      "dist-electron/**/*"
    ]
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.2",
    "axios": "^1.6.8",
    "dayjs": "^1.11.11",
    "element-plus": "^2.8.8",
    "pinia": "^2.1.7",
    "vue": "^3.5.13",
    "vue-router": "^4.3.3"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@vitejs/plugin-vue": "^5.2.1",
    "electron": "^40.4.0",
    "electron-builder": "^26.7.0",
    "typescript": "^5.7.2",
    "vite": "^6.0.7",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6"
  }
}
